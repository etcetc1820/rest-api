{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "quotes": [2, "double", "avoid-escape"],
        "no-console": "off",
        "comma-dangle": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off"
    }
}
